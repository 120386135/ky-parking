.main-box {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.img-box {
  display: flex;
  text-align: center;
  background-color: #6a5acd;
  height: 250rpx;
}

.index-img {
  width: 90%;
  height: 350rpx;
  margin: 0 auto;
  border-radius: 2%;
  margin-top: 30rpx;
}

.appointment-box {
  width: 84%;
  height: 260rpx;
  margin: 0 auto;
  border-radius: 2%;
  background-color: #9370db;
  margin-top: 50rpx;
  box-shadow: 0rpx 0rpx 0rpx 4rpx rgb(202, 178, 178);
  font-family: "微软雅黑";
}

.word-a {
  font-size: 32rpx;
  font-weight: bold;
  margin-left: 15rpx;
  margin-top: 30rpx;
  color: white;
  font-family: "微软雅黑";
}

.word-b {
  font-size: 28rpx;
  font-weight: bold;
  margin-left: 15rpx;
}

.manage {
  width: 85%;
  height: 65rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  padding-top: 20rpx;
  border-bottom: 0.5rpx #ccc solid;
  border-top: 0.5rpx #ccc solid;
}

.parking-img {
  width: 10%;
  height: 50rpx;
}

.word-parking {
  font-size: 32rpx;
  margin-left: 15rpx;
  color: #ccc;
}

.arrow {
  width: 10px;
  height: 10px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  position: absolute;
  right: 110rpx;
  transform: rotate(45deg);
  margin-top: 10rpx;
}

.parking {
  width: 85%;
  height: 65rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
  border-bottom: 1rpx #ccc solid;
}

.manage-img {
  width: 10%;
  height: 50rpx;
}

.amazing-box {
  /* background-image: url('../../images/shengyu.jpg'); */
  width: 84%;
  height: 300rpx;
  margin: 0 auto;
  border-radius: 2%;
  background-color: #9370db;
  margin-top: 140rpx;
  box-shadow: 0rpx 0rpx 0rpx 4rpx rgb(202, 178, 178);
  font-family: "微软雅黑";
}

.bottom-box {
  width: 100%;
  text-align: center;
  font-size: 22rpx;
  margin-top: 50rpx;
  text-decoration: underline;
  position: fixed;
  bottom: 15rpx;
}

.word-bottom {
  color: #9370db;
  text-decoration: underline;
}

.surplus-box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  color: white;
  padding-top: 50rpx;
  padding-left: 130rpx;
}

.liang {
  font-family: "微软雅黑";
  padding-top: 100rpx;
  padding-left: 20rpx;
  font-size: 32rpx;
}

.number-img image {
  width: 120rpx;
  height: 140rpx;
  border-radius: 5%;
}

.number {
  display: flex;
  flex-direction: row;
  height: 140rpx;
  border: 1px white solid;
  border-radius: 8%;
}
